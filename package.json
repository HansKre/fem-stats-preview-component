{
  "name": "fem-stats-preview-component",
  "description": "This is a solution to the Stats Preview Card Component challenge on Frontend Mentor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prod": "npm run build && serve -s build",
    "test:testing-library:run": "react-scripts test --watchAll=false",
    "test:testing-library:watch": "react-scripts test",
    "test:cypress:open": "(npm run prod &) && (cypress open)",
    "test:cypress:run": "(npm run prod &) && (cypress run --browser chrome)",
    "test": "npm run test:testing-library:run && npm run test:cypress:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "github",
    "url": "git@github.com:HansKre/fem-stats-preview-component.git"
  },
  "license": "ISC"
}
